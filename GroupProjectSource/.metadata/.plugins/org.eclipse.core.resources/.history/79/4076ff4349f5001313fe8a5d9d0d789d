package za.ac.wits.elen7045.group3.aps.domain.accounts.repository;

import java.util.LinkedList;
import java.util.List;

import za.ac.wits.elen7045.group3.aps.domain.accounts.abtracts.AbstractAccounts;

public class AccountRepository implements AccountDat{
	
	LinkedList<AbstractAccounts> data = new LinkedList<AbstractAccount>();
	
	@Override
	public boolean doesAccountEntryExist(AbstractAccounts account){
		return data.contains(account);
	}
	
	@Override
	public void addAccount(int index, AbstractAccount account){	
		data.add(index, account);		
	}

	@Override
	public List<AbstractAccount> getAccounts() {
		return data;
	}

}
